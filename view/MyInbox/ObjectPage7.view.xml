                    <m:Label text="Cloud Platform Integration Configuration"/>
                    <m:Text text="Donna  Moore"/>
                    <m:Label text="DB Configuration"/>
                    <m:Text text="Sabine Mayer"/>
                  </f:SimpleForm>
                </m:VBox>
              </ObjectPageLazyLoader>
            </blocks>
          </ObjectPageSubSection>
        </subSections>
      </ObjectPageSection>
      <ObjectPageSection>
        <subSections>
          <ObjectPageSubSection id="Section7-2" mode="Expanded" title="Parameters">
            <blocks>
              <ObjectPageLazyLoader id="Loader2" stashed="true">
                <m:VBox class="sapUiSmallMargin">
                  <f:SimpleForm title="Cloud Platform Integration" class="sapUiSmallMarginBottom" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="3" labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="4" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="1" columnsM="1" singleContainerFullSize="false">
